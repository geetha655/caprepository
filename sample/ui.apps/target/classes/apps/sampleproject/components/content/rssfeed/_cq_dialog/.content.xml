<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="RSS Feed"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="en/cq/current/wcm/default_components.html#List">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <title
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Title"
                        name="./title"/>
                    <spacebelow
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                        fieldLabel="Space below"
                        name="./space">
                        <items jcr:primaryType="nt:unstructured">
                            <def
                                jcr:primaryType="nt:unstructured"
                                text="(default)"
                                value=""/>
                            <none
                                jcr:primaryType="nt:unstructured"
                                text="None"
                                value="space-below_none"/>
                            <normal
                                jcr:primaryType="nt:unstructured"
                                text="Normal"
                                value="space-below_normal"/>
                            <big
                                jcr:primaryType="nt:unstructured"
                                text="Big"
                                value="space-below_big"/>
                        </items>
                    </spacebelow>
                    <feeds
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Feeds"
                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                        <items jcr:primaryType="nt:unstructured">
                            <allNews
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./allNews"
                                text="All news"
                                value="true"/>
                            <notifiableTrading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./notifiableTrading"
                                text="Notifiable trading"
                                value="true"/>
                            <remit
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./remit"
                                text="REMIT"
                                value="true"/>
                            <stockMarketAnnouncements
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./stockMarketAnnouncements"
                                text="Stock market announcements"
                                value="true"/>
                        </items>
                    </feeds>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
